<ui:UiBinder
        xmlns:ui='urn:ui:com.google.gwt.uibinder'
        xmlns:g='urn:import:com.google.gwt.user.client.ui'
        xmlns:t='urn:com.google.gwt.user.cellview.client'>
    <g:DialogBox ui:field="dialog">
        <g:DockLayoutPanel unit="EM" g:width="300px" g:height="300px">
            <g:north size="2">
                <g:Label>Value</g:Label>
            </g:north>
            <g:north size="2">
                <g:Label ui:field="result"/>
            </g:north>
            <g:north size="5">
                <t:CellTable ui:field="table"/>
            </g:north>
            <g:south size="2">
                <g:HTMLPanel>
                    <table>
                        <tr>
                            <td>
                                <g:PushButton ui:field="closeBtn">Close</g:PushButton>
                            </td>
                            <td>
                                <g:PushButton>OK</g:PushButton>
                            </td>
                        </tr>
                    </table>
                </g:HTMLPanel>
            </g:south>
        </g:DockLayoutPanel>
    </g:DialogBox>
</ui:UiBinder>